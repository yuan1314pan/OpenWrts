# Root filesystem images
CONFIG_TARGET_ROOTFS_EXT4FS=y
CONFIG_TARGET_KERNEL_PARTSIZE=64
CONFIG_TARGET_ROOTFS_PARTSIZE=800

# Ipv6 Support
CONFIG_IPV6=y
CONFIG_PACKAGE_ipv6helper=y

# Wireguard Support
CONFIG_PACKAGE_luci-proto-wireguard=y

